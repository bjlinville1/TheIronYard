<h3>Create a frog with which you can blog!</h3>

<form action="/create_a_frog" method="post">
  <label for="user_name">User Name</label>  
  <input type="text" id="user_name" name="frog[user_name]">
  <br>
  <label for="password">Password</label>
  <input type="text" id="password" name="frog[password]">
  <br>
  <label for="blog">Blog</label>
  <input type="text" id="blog" name="frog[blog]">
  <br>
  <input type="submit" value="Create Frog">
  <!-- { frog: { user_name: "" } } -->
</form>

